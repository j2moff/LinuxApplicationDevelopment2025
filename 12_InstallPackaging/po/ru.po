#: src/romanizer.c:37
#, c-format
msgid "Usage: romanizer [INFILE OUTFILE]\n"
msgstr "Использование: romanizer [INFILE OUTFILE]\n"

#: src/romanizer.c:38
#, c-format
msgid "Arguments:\n"
msgstr "Аргументы:\n"

#: src/romanizer.c:39
#, c-format
msgid "  INFILE  - cyrillic text input file\n"
msgstr "  INFILE  - входной файл с текстом на кириллице\n"

#: src/romanizer.c:40
#, c-format
msgid "  OUTFILE - latin text output file\n"
msgstr "  OUTFILE - выходной файл с текстом на латинице\n"

#: src/romanizer.c:41
#, c-format
msgid ""
"If INFILE and OUTFILE are not provided, STDIN and STDOUT will be used "
"respectively.\n"
msgstr ""
"При отсутствии INFILE и OUTFILE будут использоваться STDIN и STDOUT "
"соответственно.\n"

#: src/romanizer.c:70
#, c-format
msgid "got wrong number of arguments\n"
msgstr "получено неправильное количество аргументов\n"

#: src/romanizer.c:76
#, c-format
msgid "Write your cyrillic text:\n"
msgstr "Введите свой текст на кириллице:\n"

#: src/romanizer.c:77
#, c-format
msgid "(Press Enter or Ctrl+D to get the romanized text)\n"
msgstr "(Нажмите Enter или Ctrl+D для вывода романизированного текста)\n"

#: src/romanizer.c:78
#, c-format
msgid "(Press Enter+Ctrl+D or Ctrl+D twice to quit)\n"
msgstr "(Нажмите Enter+Ctrl+D или Ctrl+D дважды для выхода)\n"

#: src/romanizer.c:94
#, c-format
msgid "got the same input and output file\n"
msgstr "получены одинаковый входной и выходной файл\n"

#: src/romanizer.c:101
#, c-format
msgid "failed to open the input file\n"
msgstr "произошла ошибка при открытии входного файла\n"

#: src/romanizer.c:108
#, c-format
msgid "failed to open the output file\n"
msgstr "произошла ошибка при открытии выходного файла\n"
