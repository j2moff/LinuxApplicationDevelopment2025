#: src/guess.c:22
msgid "Think of a number between 1 and 100.\n"
msgstr "Придумайте число от 1 до 100.\n"

#: src/guess.c:28
#, c-format
msgid "I think your number is %d\n"
msgstr "Я думаю, что Ваше число - %d\n"

#: src/guess.c:32
#, c-format
msgid "Is your number bigger than %d? (y/n): "
msgstr "Ваше число больше чем %d? (д/н): "

#: src/guess.c:35
msgid "failed to get response\n"
msgstr "произошла ошибка при получении ответа\n"

#: src/guess.c:46
msgid "got invalid response, it should be either 'y' or 'n'\n"
msgstr "был получен некорректный ответ, он должен быть либо 'д' либо 'н'\n"

msgid "y"
msgstr "д"

msgid "n"
msgstr "н"
